{
    "name": "pixelycia/yii2-saml",
    "description": "Connect Yii 2 application to a Saml Identity Provider for Single Sign On",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "saml",
        "single sign on"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Erick Pavlov",
            "email": "pixelycia@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii2": "*",
        "onelogin/php-saml": ">=2.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.*"
    },
    "autoload": {
        "psr-4": {
            "pixelycia\\yii2saml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pixelycia\\yii2saml\\tests\\": "tests/"
        }
    }
}
